<Window x:Class="arrayfactor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:datamodel="clr-namespace:DataModel" d:DataContext="{d:DesignInstance Type=datamodel:DataModel}"
        xmlns:local="clr-namespace:arrayfactor"
        mc:Ignorable="d"
        Title="MainWindow" 
        Height="720"
        Width="1280"
        MinHeight="720"
        MinWidth="1280"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanResizeWithGrip"
        Background="Transparent"
        Icon="bin\Debug\net6.0-windows\Assets\antenna.png"
        AllowsTransparency="True">

    <Window.Resources>

        <!--// Text font style //-->

        <Style x:Key="font_style" TargetType="TextBlock">

            <Setter Property="FontFamily"
                    Value="Fonts/Roboto-Light.ttf #Roboto"/>

            <Setter Property="VerticalAlignment"
                    Value="Center"/>

            <Setter Property="HorizontalAlignment"
                    Value="Left"/>

            <Setter Property="Foreground"
                    Value="#FF313485"/>

            <Setter Property="FontSize"
                    Value="20"/>

        </Style>

        <!--// Tooltip Style Template //-->

        <Style TargetType="ToolTip" x:Key="tt_style">

            <Setter Property="Placement" Value="Right"/>
            <Setter Property="Height" Value="40"/>
            <Setter Property="Width" Value="120"/>
            <Setter Property="Margin" Value="20,0,0,0"/>
            <Setter Property="Background" Value="#FF1E1E1E"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontFamily" Value="Fonts/Roboto-Light.ttf #Roboto"/>
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="PlacementRectangle" Value="60,0,0,0"/>
            <Setter Property="HorizontalOffset" Value="15"/>
            <Setter Property="VerticalOffset" Value="6"/>
            <Setter Property="HasDropShadow" Value="True"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>

        </Style>

    </Window.Resources>

    <Grid>

        <Grid>

            <Grid.RowDefinitions>
                <RowDefinition Height="80"/>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <Grid Grid.Row="0"
                  Grid.ColumnSpan="4">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <Border Panel.ZIndex="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="3"
                        Background="Transparent"
                        MouseDown="Border_MouseDown"/>

                <Border Grid.Row="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="3"
                        CornerRadius="14,14,0,0">

                    <Border.Background>
                        <LinearGradientBrush>
                            <GradientStop Color="White" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>

                </Border>

                <ToggleButton Panel.ZIndex="1"
                              Grid.Row="1" 
                              Grid.Column="1"
                              HorizontalAlignment="Left"
                              Cursor="Hand"
                              IsChecked="True"
                              Margin="5,5,0,40">

                    <ToggleButton.Style>
                        <Style TargetType="ToggleButton">
                            <Setter Property="Background" Value="DarkCyan"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="Cyan"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ToggleButton.Style>

                    <ToggleButton.Template>

                        <ControlTemplate TargetType="ToggleButton">

                            <Border Height="40" 
                                    Width="40"
                                    VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    Margin="0,0,0,0"
                                    CornerRadius="0,0,0,0">

                                <Border.Background>
                                    <ImageBrush ImageSource="bin\Debug\net6.0-windows\Assets\antenna.png"/>
                                </Border.Background>

                                <ContentPresenter VerticalAlignment="Stretch"
                                                  HorizontalAlignment="Stretch"/>
                            </Border>
                        </ControlTemplate>
                    </ToggleButton.Template>

                    <ToggleButton.ToolTip>
                        <ToolTip Content="yusufozdemir"
                                 Style="{StaticResource tt_style}"/>
                    </ToggleButton.ToolTip>

                    <ToggleButton.Triggers>

                        <EventTrigger RoutedEvent="ToggleButton.Checked">

                            <BeginStoryboard>

                                <Storyboard>

                                    <DoubleAnimation Storyboard.TargetName="Menu"
                                                     Storyboard.TargetProperty="Width"
                                                     BeginTime="0:0:0"
                                                     From="0" To="320"
                                                     Duration="0:0:0.3">

                                    </DoubleAnimation>

                                </Storyboard>

                            </BeginStoryboard>

                        </EventTrigger>

                        <EventTrigger RoutedEvent="ToggleButton.Unchecked">

                            <BeginStoryboard>

                                <Storyboard>

                                    <DoubleAnimation Storyboard.TargetName="Menu"
                                                     Storyboard.TargetProperty="Width"
                                                     BeginTime="0:0:0"
                                                     From="320" To="0"
                                                     Duration="0:0:0.3">

                                    </DoubleAnimation>

                                </Storyboard>

                            </BeginStoryboard>

                        </EventTrigger>

                    </ToggleButton.Triggers>

                </ToggleButton>

                <Button Panel.ZIndex="1"
                        Content="Ana Menü"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="55,0,0,30"
                        PreviewMouseLeftButtonDown="HomePanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="100" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Anten"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="155,0,0,30"
                        PreviewMouseLeftButtonDown="AntennaPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="65" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Yapay Zeka"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="220,0,0,30"
                        PreviewMouseLeftButtonDown="AiPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="105" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Optimizasyon"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="325,0,0,30"
                        PreviewMouseLeftButtonDown="OptimizationPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="120" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Grafikler"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="445,0,0,30"
                        PreviewMouseLeftButtonDown="GraphicsPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="80" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Ayarlar"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="525,0,0,30"
                        PreviewMouseLeftButtonDown="SettingsPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="75" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Content="Hakkında"
                        FontSize="18"
                        FontFamily="Times New Romance"
                        FontWeight="Light"
                        Foreground="Black"
                        Grid.Row="1" 
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        Margin="600,0,0,30"
                        PreviewMouseLeftButtonDown="AboutPanel_Click">

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="90" Height="20"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Grid.Column="2"
                        HorizontalAlignment="Right"
                        Cursor="Hand"
                        Margin="0,0,90,20"
                        Click="MinimizeButton_Click">

                    <Image Source="bin\Debug\net6.0-windows\Assets\minimize.png"/>

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="20" Height="20"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Stretch"
                                                  HorizontalAlignment="Stretch"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Grid.Column="2"
                        HorizontalAlignment="Right"
                        Cursor="Hand"
                        Margin="0,0,55,20"
                        Click="ResizeButton_Click">

                    <Image Source="bin\Debug\net6.0-windows\Assets\rectangle.png"/>

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="20" Height="20"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Stretch"
                                                  HorizontalAlignment="Stretch"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <Button Panel.ZIndex="1"
                        Grid.Column="2"
                        HorizontalAlignment="Right"
                        Cursor="Hand"
                        Margin="0,0,20,20" 
                        Click="CloseButton_Click">

                    <Image Source="bin\Debug\net6.0-windows\Assets\close.png"/>

                    <Button.Style>
                        <Style TargetType="Button">
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="Opacity" Value="0.7"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>

                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border Width="20" Height="20"
                                    CornerRadius="0,0,0,0"
                                    Background="{TemplateBinding Background}">
                                <ContentPresenter VerticalAlignment="Stretch"
                                                  HorizontalAlignment="Stretch"/>
                            </Border>
                        </ControlTemplate>
                    </Button.Template>

                </Button>

                <TextBlock x:Name="arrayfactor"
                           Grid.Column="1"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Right"
                           Padding="3"
                           FontSize="30"
                           FontWeight="Bold"
                           FontFamily="Bauhaus 93"
                           Opacity="1"
                           Text="a r r a y f a c t o r"
                           Margin="0,0,150,0">

                    <TextBlock.Foreground>
                        <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                            <GradientStop Color="#FFA4D6F7" Offset="0.1"/>
                            <GradientStop Color="#FF2C427A" Offset="1"/>
                        </LinearGradientBrush>
                    </TextBlock.Foreground>

                    <TextBlock.Effect>

                        <DropShadowEffect ShadowDepth="1"
                                          Direction="330"
                                          Color="#FF1F3969"
                                          Opacity="1"
                                          BlurRadius="1"/>

                    </TextBlock.Effect>

                </TextBlock>

            </Grid>

            <Grid Grid.Row="1"
                  Grid.ColumnSpan="4">

                <Border CornerRadius="0,0,14,0">

                    <Border.Background>
                        <LinearGradientBrush>
                            <GradientStop Color="White" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>

                </Border>

            </Grid>

            <Grid x:Name="Menu"
                  Grid.RowSpan="2"
                  Grid.Column="0"
                  Width="320"
                  Margin="0,60,0,40">

                <Border Grid.Column="1"
                        Grid.ColumnSpan="1"                        
                        CornerRadius="40"
                        BorderThickness="1"
                        BorderBrush="#FF313485"
                        Margin="40,0,0,0">

                    <Border.Style>
                        <Style TargetType="Border">

                        </Style>
                    </Border.Style>

                    <Border.Background>
                        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1">
                            <GradientStop Color="White" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>

                    <Border.Effect>
                        <DropShadowEffect BlurRadius="15"
                                          ShadowDepth="15"
                                          Direction="330"
                                          Opacity="0"
                                          Color="#FF1F3969"/>
                    </Border.Effect>

                </Border>

                <Grid>

                    <TextBlock Grid.Row="1"
                               Grid.Column="0"
                               Margin="75,25,0,0"
                               Text="M E N U"
                               FontSize="35"
                               Background="Transparent"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               FontWeight="UltraBold"
                               FontFamily="Calibri"
                               Opacity="1">

                        <TextBlock.Foreground>
                            <LinearGradientBrush StartPoint="0,1" EndPoint="1,1">
                                <GradientStop Color="#FFFCE0A2" Offset="1"/>
                            </LinearGradientBrush>
                        </TextBlock.Foreground>

                        <TextBlock.Effect>

                            <DropShadowEffect ShadowDepth="1"
                                              Direction="0"
                                              Color="#FF3E3885"
                                              Opacity="0"
                                              BlurRadius="1"/>

                        </TextBlock.Effect>

                    </TextBlock>


                    <TextBlock Text="V. Açık Beta 0.9.11"                           
                           Grid.Row="1"
                           Grid.Column="1"
                           FontSize="10"
                           FontWeight="ExtraLight"
                           FontFamily="Calibri Light"
                           Foreground="Gray"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Top"
                           Opacity="1"
                           Margin="205,33,0,0">

                    </TextBlock>

                    <TextBlock Text="Tasarlayan: yusufozdemir"                           
                           Grid.Row="1"
                           Grid.Column="1"
                           FontSize="10"
                           FontWeight="ExtraLight"
                           FontFamily="Calibri Light"
                           Foreground="Gray"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Top"
                           Opacity="1"
                           Margin="205,47,0,0">

                    </TextBlock>

                    <StackPanel x:Name="st_pnl"
                                VerticalAlignment="Top"
                                Margin="55,80,0,0">

                        <ListView x:Name="LV"
                                  Background="Transparent"
                                  BorderBrush="Transparent"
                                  ScrollViewer.HorizontalScrollBarVisibility="Disabled">

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="HomePanel_Click"
                                          Margin="0,10,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\home.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Ana Menü"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="AntennaPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\antenna2.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Antenler"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="AiPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\ai.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Yapay Zeka"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="OptimizationPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\optimization.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Optimizasyon"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="GraphicsPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\graphic.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Grafikler"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="SettingsPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\settings.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Ayarlar"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="AboutPanel_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\about.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Hakkında"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListViewItem HorizontalAlignment="Left"
                                          PreviewMouseLeftButtonDown="CloseButton_Click"
                                          Margin="0,0,0,15">

                                <StackPanel Orientation="Horizontal"
                                            Width="230">

                                    <Image Source="bin\Debug\net6.0-windows\Assets\close2.png"
                                           Stretch="Uniform"
                                           Margin="12,0,0,0"/>

                                    <TextBlock Text="Çıkış"
                                               Margin="25,0,0,0"
                                               Style="{StaticResource font_style}"/>

                                </StackPanel>

                            </ListViewItem>

                            <ListView.ItemContainerStyle>

                                <Style TargetType="ListViewItem">

                                    <Setter Property="Template">

                                        <Setter.Value>

                                            <ControlTemplate TargetType="{x:Type ListViewItem}">

                                                <Border x:Name="Bd"
                                                        Height="50"
                                                        CornerRadius="10"
                                                        BorderBrush="{TemplateBinding BorderBrush}"
                                                        BorderThickness="{TemplateBinding BorderThickness}"
                                                        Background="{TemplateBinding Background}"
                                                        Padding="{TemplateBinding Padding}"
                                                        SnapsToDevicePixels="True"
